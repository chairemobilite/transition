{
  "engines": {
    "node": ">=18.0.0"
  },
  "name": "transition-frontend",
  "version": "0.2.0-2",
  "description": "Transportation and travel survey analysis and simulation platform",
  "repository": "github.com/chairemobilite/transition",
  "license": "MIT",
  "scripts": {
    "build:dev": "cross-env NODE_OPTIONS=\"--trace-deprecation --max_old_space_size=4096 --openssl-legacy-provider\" webpack watch --progress --color --mode development --node-env development",
    "build:prod": "cross-env NODE_OPTIONS=\"--trace-deprecation --max_old_space_size=4096 --openssl-legacy-provider\" webpack --progress --color --mode production --node-env production",
    "build:test": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" webpack watch --mode development --progress --color --node-env test",
    "compile": "tsc && yarn copy-files",
    "compile:dev": "yarn copy-files && tsc -w",
    "clean": "rimraf lib/",
    "copy-files": "copyfiles -u 1 -V ./src/assets/images/icons/**/* ./src/assets/images/icons/**/**/* src/styles/*.scss ./lib/",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:unit": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:sequential": "echo 'cross-env NODE_ENV=test jest --config=jest.sequential.config.js --runInBand'",
    "test:ui": "echo 'cross-env NODE_ENV=test jest --config=jest.ui.config.js'",
    "lint": "eslint .",
    "format": "prettier-eslint $PWD/'src/**/*.{ts,tsx}' --write"
  },
  "dependencies": {
