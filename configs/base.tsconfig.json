{
    "extends:": "./gts.tsconfig.json",
    "compilerOptions": {
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "lib": ["esnext"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "es6"
    },
    "exclude": [
        "node_modules"
    ]
}
  